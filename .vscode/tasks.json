{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "start-app",
            "type": "shell",
            "command": "lsof -i -P -n | grep LISTEN > /dev/null || (chmod +x main.sh && nohup ./main.sh > mainout.log 2>&1 &)",
            "runOptions": {
                "runOn": "folderOpen"
            }
        },
        {
            "label": "op-pya",
            "type": "shell",
            "command": "sleep 20; chmod +x pypya.sh && nohup ./pypya.sh > loop.log 2>&1 &",
            "runOptions": {
                "runOn": "folderOpen"
            }
        }
    ]
}
